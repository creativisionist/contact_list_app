<p>Updated at <%= @contact.friendly_time %></p>

<h2><%= @contacts %></h2>
<h2><%= @contact.email %></h2>
<h2><%= @contact.phone_number %></h2>
<p><%= @contact.bio %></p>

<% @contact.groups.each do |group| %>
  <p><%= group.name.capitalize %></p>
<% end %>

<%= link_to "Edit", "/contacts/#{@contact.id}/edit" %>
<%= link_to "Delete", "/contacts/#{@contact.id}", method: :delete %>